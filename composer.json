	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "terrestris/IMP",
				"version": "0.1",
				"dist": {
					"url": "https://github.com/terrestris/IMP/archive/master.zip",
					"type": "zip"
				},
                "autoload": {
                    "classmap": ["classes/"]
                }
			}
		}
	],
	"require": {
		"laravel/laravel": "4.2.*",
		"phayes/geophp": "1.2",
		"mf2/mf2": "0.2.*",
		"terrestris/IMP": "0.1",
		"socialism/laravel-opauth": "0.1.*",
		"opauth/opauth": "0.4.*",
		"opauth/github": "0.1.*",
		"opauth/twitter": "0.3.*",
		"opauth/google": "0.2.*",
		"opauth/facebook": "0.2.*"
	},
	"require-dev": {
		"phpunit/phpunit": "4.*"
	},
	"autoload": {
		"psr-0": {
			"GeoMetadata\\": "app/"
		},
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
